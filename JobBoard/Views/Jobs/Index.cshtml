@model IEnumerable<JobBoard.Models.Jobs>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Job_Title)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Job_Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Job_CreatedAt)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Job_ExpiresAt)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Job_Comments)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Job_Title)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Job_Description)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Job_CreatedAt)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Job_ExpiresAt)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Job_Comments)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
            @Html.ActionLink("Details", "Details", new { id=item.id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id })
        </td>
    </tr>
}

</table>
